      <ExcludeAssets>contentFiles</ExcludeAssets>
    </PackageReference>
    <PackageReference Include="Argu" Version="6.1.1" />
  </ItemGroup>
  
  <ItemGroup>
    <Compile Include="Data.fs" />
    <Compile Include="Parsers.fs" />
    <Compile Include="Report.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>

</Project>
