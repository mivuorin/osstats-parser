<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <IsPackable>false</IsPackable>
    <PackageId>OsStatsParser</PackageId>
    <Authors />
    <Company>OsStatsParser</Company>
    <Product>OsStatsParser</Product>
  </PropertyGroup>

  <ItemGroup>
